{
    "name": "wickedone/phpunit-printer",
    "description": "phpunit output with phpstorm editor url",
    "type": "library",
    "keywords": [
        "phpunit",
        "phpstorm",
        "testing"
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "phpunit/phpunit": "^9.6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wickedOne",
            "email": "wicliff.wolda@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WickedOne\\PHPUnitPrinter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WickedOne\\PHPUnitPrinter\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "infection/infection": "^0.26"
    },
    "conflict": {
        "phpunit/phpunit": "^10.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
